<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="a1" class="com.iocpack.Address">
		<constructor-arg value="hyderabad"></constructor-arg>
		<constructor-arg value="Telengana"></constructor-arg>
		<constructor-arg value="India"></constructor-arg>
	</bean>


	<bean id="e" class="com.iocpack.Employee">
		<constructor-arg>
			<ref bean="a1" />
		</constructor-arg>
		<constructor-arg value="12" type="int"></constructor-arg>
		<constructor-arg value="Bharadwaj"></constructor-arg>
	</bean>
</beans>  